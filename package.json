    "history": "^4.7.2",
    "jsonwebtoken": "^8.4.0",
    "node-fetch": "^2.3.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "sequelize": "^4.42.0",
    "serialize-javascript": "^1.5.0",
    "source-map-support": "^0.5.9",
    "sqlite3": "^4.0.4",
    "styled-components": "^4.1.2",
    "styled-normalize": "^8.0.4",
    "universal-router": "^7.0.0",
    "whatwg-fetch": "^3.0.0"
